# Node.js specific exclusions

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Dependency directories
node_modules/

# Environment variable files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Directories for test coverage reports
coverage/

# Optional npm cache directory
.npm/

# Optional eslint cache
.eslintcache

# System files
.DS_Store
Thumbs.db

# Visual Studio Code specific exclusions
.vscode/
.history/

# Ignore build output directories
build/
dist/
out/

# Ignore local MongoDB database files
/data/db/

# Optional directories used by some build tools
tmp/
temp/

# Ignore any custom server or secret files
server.config.js
secret.json

# Ignore lock files for yarn and npm (if you prefer to use one over the other)
yarn.lock
package-lock.json

# Ignore specific IDE or text editor project files
.idea/
*.sublime-workspace
*.sublime-project

# Ignore files generated by debugging
*.pid

# Ignore any coverage directories or files
.nyc_output/

# Ignore files generated by linter or formatter
*.cache
.prettierignore

# Optional: Exclude all logs and databases from the 'public/uploads' directory
public/uploads/logs/
public/uploads/db/

# Optional: Exclude compiled and transpiled files (JavaScript, CSS, etc.)
*.map
*.compiled.js
*.compiled.css
